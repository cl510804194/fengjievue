package com.yjf.esupplier.ws.info;

public class DivisionRuleInfo {
	private long ruleId;
	
	private int roleId;
	
	private long ruleTemplateId;
	
	private double divisionRule;
	
	public long getRuleId() {
		return this.ruleId;
	}
	
	public void setRuleId(long ruleId) {
		this.ruleId = ruleId;
	}
	
	public int getRoleId() {
		return this.roleId;
	}
	
	public void setRoleId(int roleId) {
		this.roleId = roleId;
	}
	
	public long getRuleTemplateId() {
		return this.ruleTemplateId;
	}
	
	public void setRuleTemplateId(long ruleTemplateId) {
		this.ruleTemplateId = ruleTemplateId;
	}
	
	public double getDivisionRule() {
		return this.divisionRule;
	}
	
	public void setDivisionRule(double divisionRule) {
		this.divisionRule = divisionRule;
	}
	
	@Override
	public String toString() {
		StringBuilder builder = new StringBuilder();
		builder.append("DivisionRuleInfo [ruleId=");
		builder.append(ruleId);
		builder.append(", roleId=");
		builder.append(roleId);
		builder.append(", ruleTemplateId=");
		builder.append(ruleTemplateId);
		builder.append(", divisionRule=");
		builder.append(divisionRule);
		builder.append("]");
		return builder.toString();
	}
	
}
