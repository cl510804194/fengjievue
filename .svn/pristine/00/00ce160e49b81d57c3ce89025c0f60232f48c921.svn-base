/****************************************************
 * 功能  module
 * 
 * 前缀  .m-
 ****************************************************
 */
/**
 * viewport
 */
.m-vp{
  min-width: 1300px;
}
.m-content{
  padding:25px 20px 25px 0;
}
/**
 * header
 */
.m-hd{
  background: #fdfdfd;
  border-bottom:3px solid #25619a;
}
.m-hd .in{
  padding: 0 0 0 10px;
}
.m-hd .u-logo{
  float: left;
  margin: 25px 20px 0 0;
}
.m-hd .u-logo a{font-size: 18px; font-family: 微软雅黑; color:#333;}
.m-hd .m-nav{
  float: left;
  margin: 40px 0 -3px;
}

/**
 * 导航栏
 */
.m-nav .links{
  overflow: hidden;
}
.m-nav .links li{
  float: left;
  height: 34px;
  width: 105px;
  margin: 0 5px 0 0;
  line-height: 30px;
  text-align: center;
  background: url(images/nav_link.png) no-repeat center top;
}
.m-nav .links a{
  display: block;
  height: 100%;
}
.m-nav .links a:hover{
  color: #417eb7;
}
.m-nav .links .active{
  background: url(images/nav_link_active.png) no-repeat center top;
  color: #fff;
}
.m-nav .links .active a{
  color: #fff;
}

/**
 * head bottom
 */
.m-hdBt{
  background:url(images/header_bottom.jpg) repeat-x 0 0;
  border-bottom: 1px solid #d6d6d6;
}
.m-hdBt .in{
  height: 33px;
  line-height: 33px;
  padding: 0 30px;
}
.m-hdBt .tool{
  float: right;
}

/**
 * 欢迎语
 */
.m-great{
  color: #6d6d6d;
}

/**
 * 菜单
 */
.m-menu{
  overflow: auto;
}
.m-menu .title{
  height: 36px;
  line-height: 36px;
  padding: 0 10px;
  /**margin: 20px 0 0 0;*/
  overflow: hidden;
  color: #000;
  background: url(images/menu_title.png) repeat-x 0 0;
}
.m-menu .title .txt{
  vertical-align: middle;
}
.m-menu .title .tool.collapse{
  float: right;
  width: 13px;
  height: 10px;
  margin:13px 0 0;
  background: url(images/icon_collapse.jpg) no-repeat center center;
}

.m-menu .list .subMenuList{
	display: block;
}

.m-menu .list a{
  display: block;
  height: 30px;
  line-height: 30px;
  padding: 0 0 0 58px;
  color: #b9ddff;
  background: url(images/arrow_left.png) no-repeat 45px center;
}

.m-menu .list a.title{
  display: block;
  height: 30px;
  line-height: 30px;
  padding: 0 0 0 28px;
  color: #ffffff;
  background-image: url(images/arrow_down.png);
  background-position: 14px center;
  background-repeat: no-repeat;
}

.m-menu .list a.lv2{
  display: block;
  height: 30px;
  line-height: 30px;
  padding: 0 0 0 28px;
  background: url(images/arrow_left.png) no-repeat 15px center;
}

.m-menu .list a.title1{
  display: block;
  height: 30px;
  line-height: 30px;
  padding: 0 0 0 38px;
  color: #ffffff;
  background-image: url(images/arrow_down.png);
  background-position: 24px center;
  background-repeat: no-repeat 
}

.m-menu .list a.lv3{
  display: block;
  height: 30px;
  line-height: 30px;
  padding: 0 0 0 38px;
  background: url(images/arrow_left.png) no-repeat 25px center;
}

.m-menu .list a.title-up{
  background-image: url(images/arrow_up.png) !important;
}

.m-menu .list a.title-down{
  background-image: url(images/arrow_down.png) !important;
}


.m-menu .list .active a{
  color: #b9ddff;
  background-color: #346592;
}

.m-menu .list a:hover{
  color: #fff;
}

/**
 * 内容主容器
 */
.m-main{
  border-top: 1px solid #fff;
  border-bottom: 1px solid #171717;
}
.m-main .g-sb,
.m-main .g-mn,
.m-main .g-mnIn,
.m-main .m-menu{
  height: 100%;
}
.m-main .m-menu{
  padding-right: 9px;
}
.m-main .g-sb{
  background: #417eb7 url(images/menu_bg.jpg) repeat-y right 0;
}
.m-main .g-sb .toggleBtn{
  position: absolute;
  height: 41px;
  width: 9px;
  top: 50%;
  right: -1px;
  background: url(images/collapse.jpg) no-repeat 0 0;
}
/*折叠侧边栏*/
.m-main .g-sb,
.m-main .g-mnIn{
  -moz-transition:all .4s ease;
  -webkit-transition:all .4s ease;
  transition:all .4s ease;
}
.m-main.j-collapsed .g-sb{
  width: 7px;
  margin-right:0px;
}
.m-main.j-collapsed .g-mnIn{
  padding:0 0 0 20px;
}
.m-main.j-collapsed .m-menu{
  display: none;
}
.m-main.j-collapsed .toggleBtn{
  background: url(images/expand.jpg) no-repeat 0 0;
}

/**
 * footer
 */
.m-ft{
  height: 36px;
  border-top: 1px solid #707070;
  background: url(images/foot_bg.jpg) repeat-x 0 0;
}
.m-ft .in{
  padding: 0 30px;
}
.m-ft .copyright{
  float: right;
  margin: 10px 0 0;
}


/**
 * 表单(横向布局)
 */
.m-form.horizontal .row{
  padding: 13px 0px;
}
.m-form.horizontal .row .select{
  width: 183px;
  padding: 0 0 0 5px;
}
.m-form.horizontal .group{
  display: inline-block;
  *display: inline;
  *zoom: 1;
  width: 200px;
  padding:0 0 0 110px;
  margin: 0 20px 0 0;
}
.m-form.horizontal .group .label{
  float: left;
  width: 110px;
  padding: 3px 0 0 0;
  margin:0 0 0 -110px;
  text-align: right;
  color: #000;
}

.m-form.horizontal .group .input,
.m-form.horizontal .group .input2,
.m-form.horizontal .group .select{
  width: 170px;
  height: 25px;
  padding: 0 5px;
  border:1px solid #ccc;
  border-color: #bebebe #e1e1e1 #e1e1e1;
}
.m-form.horizontal .group .input2{
  width: 65px;
  margin-right: 5px;
}
.m-form.horizontal .group .select{
  width: 183px;
  padding: 0 0 0 5px;
}

.m-form.horizontal .group .u-btn{
  margin: 0 10px 0 0;
}

/**
 * 表单（列布局）
 */
.m-form.column{
  width: 1000px;
  padding: 15px 0 30px;
}
.m-form .col{
  float: left;
  width: 33.333%
}
.m-form .col.col2{
  float: left;
  width: 50%
}
.m-form.column .group{
  padding-left: 140px;
  margin: 15px 0 0;
  line-height: 23px;
}
.m-form.column .col.col2 .group{
  padding-left: 185px;
}
.m-form.column .tip{
  padding-left: 140px;
  color: #888;
}
.m-form.column .group .label{
  position: relative;
  float: left;
  width: 160px;
  height: 25px;
  line-height: 22px;
  padding: 1px 5px 1px 0;
  margin: 0 0 0 -130px;
  text-align: right;
  z-index: 1;
  zoom: 1;
}
.m-form.column .col.col2 .group .label{
  width: 175px;
  margin: 0 0 0 -180px;
}
.m-form.column .group .label2{
  margin:0 30px 0 0;
}
.m-form.column .group .label i{
  display: inline-block;
  *display: inline;
  *zoom: 1;
  padding: 0 5px 0 0;
  vertical-align: middle;
  color: red;
}
.m-form.column .group .input,
.m-form.column .group .select,
.m-form.column .group .select2,
.m-form.column .group .select3,
.m-form.column .group .textarea{
  width: 160px;
  height: 25px;
  padding: 0 5px;
  margin:0 5px 0 0;
  border:1px solid #ccc;
  border-color: #bebebe #e1e1e1 #e1e1e1;
}
.m-form.column .group .textarea{
  height: 75px;
}
.m-form.column .group .input.full{
  width: 412px;
}
.m-form.column .group .input2{
  width: 71px;
  margin: 0 10px 0 0;
}
.m-form.column .group .input3{
  width: 500px;
}
.m-form.column .group .select,
.m-form.column .group .select2{
  width: 173px;
  padding: 0 0 0 5px;
}
.m-form.column .group .select2{
  width: 88px;
  margin: 0 10px 0 0;
}
.m-form.column .group .radio{
  vertical-align: middle;
  margin: -2px 0 0 10px;
}
.m-form.column .group .checkbox{
  margin: -2px 0 0 0;
}
/*异常*/
.m-form.column .group.error .input{
  border-color: #c00;
  color: #c00;
}
/**
 * 工具栏
 */
.m-toolbar{
  border: 1px solid #d6d6d6;
  padding: 1px;
  background: #fff;
}
.m-toolbar .m-form{
  background:#f1f1f1;
}

/**
 * 表格
 */
.m-grid .table{
  width: 100%;
  border:1px solid #d6d6d6;
  table-layout: fixed;
}
.m-grid th{
  height: 36px;
  line-height: 36px;
  padding: 1px;
  font-weight: normal;
  color: #000;
  background: #fff;
  border:1px solid #d6d6d6;
  border-width: 0 1px 1px 0;
}
.m-grid th div{
  height: 100%;
  background: #f1f1f1;
}
.m-grid td{
  padding: 13px 0;
  line-height: 20px;
  border-bottom: 1px solid #d6d6d6;
  vertical-align: middle;
  text-align: center;
}
.m-grid .odd td{
  background: #fff;
}
.m-grid td .u-link{
  margin: 0 5px;
}

/**
 * 片段标题
 */
.m-sectionHd{
  padding: 10px;
  overflow: hidden;
  margin-bottom: 10px;
}
.m-sectionHd .center{
  float: left;
  width: 100%;
  text-align: center;
  font-size: 18px;
  font-weight: bold;
  color: #444;
}
.m-sectionHd .left,
.m-sectionHd .right{
  line-height: 28px;
  position: relative;
  z-index: 1;
}
.m-sectionHd .left{
  float: left;
  width: 50%;
  margin: 0 -50% 0 0;
}
.m-sectionHd .right{
  float: right;
  width: 50%;
  margin: 0 0 0 -50%;
  text-align: right;
}
.m-sectionHd .tool{
  display: inline-block;
  *display: inline;
  *zoom: 1;
  padding: 0 0 0 10px;
}

/**
 * 片段
 */
.m-section{
  border:1px solid #d6d6d6;
  border-width: 0 1px 1px;
}
.m-section:first-child,
.m-section.first{
  border-width: 1px;
}
.m-section .hd{
  height: 35px;
  padding: 1px;
  background: #fff;
}
.m-section .hd .in{
  padding: 10px;
  background: #f1f1f1;
}
.m-section .hd .title{
  padding: 0 0 0 25px;
  font-weight: bold;
  color: #545454;
}
.m-section .bd{
  border-top:1px solid #d6d6d6;
  background: #fff;
}

/**
 * m-loginPage
 */
.m-loginPage,
.m-loginPage body{
  height: 100%;
  min-width: 1200px;
}

/**
 * 登陆页面
 */
.m-hdWrap2{
  position: fixed;
  top:0px;
  left: 0px;
  right: 0px;
  width: 100%;
  padding: 0 0 21px 0;
  background: url(images/head_shadow.png) no-repeat center bottom;
  z-index: 1;
}
.m-hd2{
  background: url(images/head_bg.png) repeat-x 0 0;
  border-bottom: 1px solid #5984ab;
}
.m-hd2 .g-ct{
  height: 91px;
}
.m-hd2 .logo{
  float: left;
  padding: 20px 0 0 0;
}
.m-hd2 .logo a{font-size: 32px; font-weight: normal; color:#fff; font-family: 微软雅黑;}
.m-hd2 .tool{
  float: right;
  margin: 30px 0 0 0;
  color: #fff;
}
.m-hd2 .regLink{
  height: 31px;
  width: 123px;
  background: url(images/reg_link.png) no-repeat 0 0;
}

/**
 * 登陆页面 片段
 */
.m-section2{
  height: 100%;
  background: url(images/login_bg.jpg) no-repeat center top;
}

/**
 * 登陆面板
 */
.m-panelWrap{
  padding: 178px 0 50px;
  background: url(images/m-panelMirror.png) no-repeat center bottom;
}
.m-panel{
  height: 405px;
}
.m-panel .g-sb2,
.m-panel .g-mn2,
.m-panel .g-mnIn2{
  height: 100%;
}

.m-login .m-form2{
  padding: 112px 0 0 152px;
}
/*登陆*/
.m-login{
  background: url(images/loginPanel.png) no-repeat center top;
}
.m-login .g-mnIn2{
  background: url(images/loginAd.png) no-repeat 243px bottom;
}


/**
 * 登陆表单
 */
.m-form2{
  width: 273px;
}
.m-form2 .group{
  position: relative;
  zoom: 1;
  z-index: 1;
  padding: 0 0 0 38px;
  margin: 0 0 22px 0;
  border:1px solid #d7d7d7;
  background-color:#fff;
  background-repeat:no-repeat;
  background-position: 10px center; 
}
.m-form2 .group .input{
  position: relative;
  width: 100%;
  height: 20px;
  line-height: 20px;
  padding: 7px 0;
  border-width: 0;
  background: url(images/blank.png) repeat 0 0;
  z-index: 2;
}
.m-form2 .group .placeholder{
  position: absolute;
  top: 6px;
  left: 38px;
  color: #999;
  z-index: 1;
  font-family: arial,microsoft yahei;
  font-size: 14px;
}
.m-form2 .user{
  background-image:url(images/user.png);
}
.m-form2 .password{
  background-image:url(images/password.png);
}

/**
 * 注册表单
 */
.m-form3{
  padding: 42px 60px 0 87px;
}
.m-form3 .title{  
  width: 100%;
  height: 11px;
  margin-bottom: 55px;
  border-bottom:1px solid #dbdbdb;
  font-size: 16px;
  color: #5992c8;
}
.m-form3 .title span{
  float: left;
  padding: 0 10px 0 0;
  background: #fff;
}
.m-form3 .control{
  padding: 0 0 0 165px;
  margin-bottom: 22px;
}
.m-form3 .control .label{
  float: left;
  width: 155px;
  height: 35px;
  line-height: 35px;
  margin: 0 0 0 -165px;
  text-align: right;
  font-size: 13px;
  font-family:'microsoft yahei';
  font-size:14px;
}
.m-form3 .group{
  position: relative;
  display: inline-block;
  *display: inline;
  *zoom: 1;
  padding: 0 10px 0 38px;
  border:1px solid #d7d7d7;
  zoom: 1;
  z-index: 1;
}
.m-form3 .group2{
  padding: 0 10px;
}
.m-form3 .group .placeholder{
  position: absolute;
  z-index: 1;
  top: 8px;
  left: 40px;
  color:#999;
}
.m-form3 .group .input,
.m-form3 .group .input2{
  position: relative;
  height: 20px;
  width: 235px;
  line-height: 20px;
  padding: 7px 0;
  border-width: 0;
  background: url(images/blank.png) repeat 0 0;
  z-index: 2;
}
.m-form3 .group .input2{
  width: 140px;
}
.m-form3 .group2 .input2{
  width: 168px;
}
.m-form3 .tip{
  color:#999999;
  margin: 0 20px;
}
.m-form3 .control .u-btn3.full{
  width: 280px;
}
/**
 * 门店注册面板
 */
.m-regist{
  background: url(images/registPanel.png) no-repeat center top;
}
.m-regist .m-form3 .user{
  background: url(images/icon_user2.png) no-repeat 12px 9px;
}
.m-regist .m-form3 .mobile{
  background: url(images/icon_mobile.png) no-repeat 15px 9px;
}

/**
 * 页尾
 */
.m-foot{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  z-index: 2;
  padding-bottom: 30px;
}
.m-foot .g-ct{
  line-height: 25px;
  color: #fff;
  text-align: center;
}

/* 设置内联, 减少浮动带来的bug */
.fn-left,.fn-right {
	display:inline;
}
.fn-left {
	float:left;
}
.fn-right {
	float:right;
}

/* 清理浮动 */
.fn-clear:after {
	visibility:hidden;
	display:block;
	font-size:0;
	content:" ";
	clear:both;
	height:0;
}
.fn-clear {
	zoom:1; /* for IE6 IE7 */
}

ul,ol{
  list-style-type: none;
}