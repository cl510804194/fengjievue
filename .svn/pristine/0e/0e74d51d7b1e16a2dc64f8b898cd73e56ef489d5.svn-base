body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	text-align:center;	
	background-color:#FFF;
	color:#555555;
	line-height:24px;
}
a:link {
	color: #555555;
	text-decoration: none;
}
a:visited {
	color: #555555;
	text-decoration: none;
}
a:hover {
	color: #ff0000;
	text-decoration: none;
}
a:active {
	color: #555555;
	text-decoration: none;
}
