/*头部导航样式*/
.header{
    position: fixed;
    top:0;
    background-color: #222222;
    width: 100%;
    height: 100px;
    z-index: 100;
}
.header_child{
    width: 1100px;
    height: 100px;
    margin:0 auto;
    background-color: #222222;
}
.child_left{
    float: left;
    height: 100px;
    background-color: #222222;
    width: 550px;
}

.test{width:100px;height:100px;color:red;background: url('../images/common/body-bg.jpg') no-repeat;}
.child_right ul{
    margin-left: 20px;
}

.child_right li{
    position: relative;
    list-style-type: none;
    color: white;
    float: left;
    text-align: center;
    background-color: #222222;
    margin-top: 15px;
    width: 108px;
    padding-top: 15px;
    height: 70px;
    border-top-left-radius: 11px;
    border-top-right-radius: 11px;
    /*behavior: url(../../resources/js/lib/ie-css3.htc)*/
}
.child_right li .link{
	color: #fff;
	cursor: pointer;
	display: inline-block;
	width: 100%;
	height: 100%;
}
.child_right li:hover{
    background-color: #C00D13;
}
/*二级目录样式*/
.find_fengjie{
    position: absolute;
    top: 85px;
    width: 568px;
    height: 280px;
    background-color: black;
    display: none;

}
.find_right{
    padding: 8px;
    height: 263px;
    float: left;
    background-color: #2d2d2d;
}
.find_right img{
    width: 170px;
    height: 100px;
}
.find_right p{
    width: 165px;
    background-color: black;
    padding: 3px;
    font-family: "微软雅黑";
    font-size: 12px;
    color: white;
}
.find_left{
    width: 380px;
    height: 280px;
    float: left;
    background-color: black;
}
.find_left table{
    width: 100%;
    color: white;
    margin-top: 20px;
    font-family: 宋体;
    font-size: 12px;
}
.find_left table td:first-child{
    border-right: solid 1px grey;
}
.find_left table td:nth-child(2){
    border-right: solid 1px grey;
}
.find_left table tr:first-child td{
    font-size: 14px;
    color: #00a0e9;
    padding-left: 0;
    text-align: center !important;
}
.find_left table td{
    text-align: left;
    padding-left: 25px;
    height: 30px;
}
.find_left table td:hover{
    color: red;
}


/* banner */
.banner { position: relative; overflow: auto; text-align: center; overflow: hidden}
.banner li { list-style: none; }
.banner ul li { float: left; }
#b04 { margin-top: 100px}
#b04 .dots { position: absolute; left: 0; right: 0; bottom: 20px;}
#b04 .dots li
{
	display: inline-block;
	width: 10px;
	height: 10px;
	margin: 0 4px;
	text-indent: -999em;
	border: 2px solid #fff;
	border-radius: 6px;
	cursor: pointer;
	opacity: .4;
	-webkit-transition: background .5s, opacity .5s;
	-moz-transition: background .5s, opacity .5s;
	transition: background .5s, opacity .5s;
}
#b04 .dots li.active
{
	background: #fff;
	opacity: 1;
}
#b04 .arrow { position: absolute; top: 200px;}
#b04 #al { left: 15px;}
#b04 #ar { right: 15px;}

/*景点*/
.content1{
	width: 100%;
	height: 880px;
	background-color: #f5eee0;
}
.wrapper{
	width: 1100px;
	margin: 0 auto;
	overflow: hidden;
}
.welcome img{
	margin: 0 auto;
