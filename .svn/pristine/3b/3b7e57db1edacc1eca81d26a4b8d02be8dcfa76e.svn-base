.orders-wrapper {margin-top: 10px;}
.orders-wrapper .order-title {
    position: relative;
    line-height: 24px;
    margin-top: 12px;
    color: #999;
    font-size: 12px;
    zoom: 1
}

.orders-wrapper .order-title a {
    color: #999
}

.orders-wrapper .order-title .del {
    position: absolute;
    right: 0;
    top: 8px;
    padding-left: 11px;
    height: 12px;
    zoom: 1;
    overflow: hidden;
    background-position: 0 -13px
}

.orders-wrapper .order-title .del.del-hover,.orders-wrapper .order-title .del:hover {
    background-position: 0 0
}

.orders-wrapper .order-title .del:hover {
    text-decoration: none;
    background-position: 0 0
}

.orders-wrapper .order-row,.orders-wrapper .orders-head {
    background: #f9f9f9;
    overflow: hidden;
    zoom: 1;
}

.orders-wrapper .order-title .del:hover:after {
    color: #007d06
}

.orders-wrapper .order-title .del:after {
    padding-left: 3px;
    content: '删除';
    color: #ddd;
    vertical-align: top;
    line-height: 12px;
    zoom: 1;
    display: inline-block
}
.orders-wrapper.order-list {overflow:hidden;}
.orders-wrapper.order-list li{ width: 100%; float:left;  line-height: 30px;}
.orders-wrapper.order-list li label.order-titlename{ width: 100px; text-align: right; vertical-align: top; height: 30px; line-height: 30px; display: inline-block; color:#000; font-size: 13px;}
.orders-wrapper.order-list li span.order-name{ width: 600px;  text-align: left;}
.component-order-nav:after,.side-nav__list .current a:after,.side-nav__list a:before,.side-nav__list dd:after {
    content: ''
}

.orders-wrapper .deal-title,.orders-wrapper .order-op a {
    display: block
}

.orders-wrapper .order-title .time {
    padding-left: 18px
}

.orders-wrapper .orders-head {
    border-top: 1px solid #eee;
    color: #999;
    line-height: 32px;
    text-align: center
}

.orders-wrapper .orders-head .order-cell {
    border: none;
    height: 30px;
    border-top: 1px solid #fff;
    min-height: 26px;
    _height: 26px;
    line-height: 26px
}

.orders-wrapper .orders-head .order-quantity {
    width: 54px
}

.orders-wrapper .orders-head .order-money {
    width: 184px
}

.orders-wrapper td {
    padding: 10px
}

.orders-wrapper .order-row {
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
    padding: 11px 0 15px;
    color: #666;
    line-height: 20px
}

.orders-wrapper .order-row a:hover {
    text-decoration: none
}

.orders-wrapper .order-row--sub {
    float: left;
    padding-bottom: 15px;
    border-bottom: 1px solid #eee;
    margin-bottom: 11px
}

.orders-wrapper .order-row--last {
    border-bottom: 0;
    padding-bottom: 0;
    margin-bottom: 0
}

.orders-wrapper .deal-info,.orders-wrapper .order-info .pic {
    padding: 0 20px
}

.orders-wrapper .img-w {
    float: left;
    width: 70px
}

.orders-wrapper .img-w img {
    float: left;
    width: 60px;
    height: 60px;
    font-size: 0;
    overflow: hidden;
    margin-right: 15px;
}

.orders-wrapper .order-cell {
    float: left;
    text-align: center
}

.orders-wrapper .order-cell--right {
    float: right
}

.orders-wrapper .order-info {
    width: 335px
}

.orders-wrapper .info-detail {
    float: right;
    width: 195px
}

.orders-wrapper .info-detail p {
    color: #999;
    padding:0px;
    text-indent:0px;
}

.orders-wrapper .info-detail .biz-info {
    color: #007d06
}

.orders-wrapper .order-info a {
    color: #666
}

.orders-wrapper .order-info a:hover {
    color: #007d06
}

.orders-wrapper .order-info .text {
    padding: 0 20px 0 0
}

.orders-wrapper .order-row .order-info {
    text-align: left
}

.orders-wrapper .order-quantity {
    width: 52px
}

.orders-wrapper .order-money {
    width: 183px
}

.orders-wrapper .order-status {
    width: 153px;
    padding: 0 20px
}

.orders-wrapper .order-op {
    width: 110px;
    padding-right: 0px;
}

.orders-wrapper .order-op .btn-mini {
    min-width: 65px;
    margin-bottom: 7px
}

.orders-wrapper .order-op .help {
    border-radius: 6px;
    padding: 0 4px;
    background: #ddd
}

#orders .coupons-box .mainbox {
    position: relative
}

#orders .coupons-box .mainbox .pbanner {
    position: absolute;
    top: 20px;
    right: 26px
}

.filter-short {
    margin-left: 24px;
    color: #999;
    font-size: 12px
}

.filter-short .em {
    padding-left: 10px;
    color: #f76120
}

.orders-filter {
    margin-bottom: 20px;
    position: relative;
    z-index: 1;
    width: 101px
}
