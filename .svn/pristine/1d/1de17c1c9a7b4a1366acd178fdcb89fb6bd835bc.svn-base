package com.yjf.esupplier.ws.gifamount.result;

import com.yjf.common.lang.util.money.Money;
import com.yjf.esupplier.ws.result.EsupplierBaseResult;

public class GiftMoneyAssignResult extends EsupplierBaseResult {
	private Money giftAmount ;
	private Money experienceAmount;
	private Money giftCashAmount;
	private Money virtualExperienceAmount;
	private String gainAmount;

	public Money getGiftAmount() {
		return giftAmount;
	}

	public void setGiftAmount(Money giftAmount) {
		this.giftAmount = giftAmount;
	}

	public Money getExperienceAmount() {
		return experienceAmount;
	}

	public void setExperienceAmount(Money experienceAmount) {
		this.experienceAmount = experienceAmount;
	}

	public Money getGiftCashAmount() {
		return giftCashAmount;
	}

	public void setGiftCashAmount(Money giftCashAmount) {
		this.giftCashAmount = giftCashAmount;
	}

	public Money getVirtualExperienceAmount() {
		return virtualExperienceAmount;
	}

	public void setVirtualExperienceAmount(Money virtualExperienceAmount) {
		this.virtualExperienceAmount = virtualExperienceAmount;
	}

	public String getGainAmount() {
		return gainAmount;
	}

	public void setGainAmount(String gainAmount) {
		this.gainAmount = gainAmount;
	}
}
