        sb.append("giftRuleId=").append(giftRuleId);
        sb.append(", giftId=").append(giftId);
        sb.append(", giftRuleName='").append(giftRuleName).append('\'');
        sb.append(", ruleType=").append(ruleType);
        sb.append(", type='").append(type).append('\'');
        sb.append(", status='").append(status).append('\'');
        sb.append(", rule='").append(rule).append('\'');
        sb.append(", ruleAmount='").append(ruleAmount).append('\'');
        sb.append(", amount='").append(amount).append('\'');
        sb.append(", createUserid=").append(createUserid);
        sb.append(", createUsername='").append(createUsername).append('\'');
        sb.append(", rawAddTime=").append(rawAddTime);
        sb.append(", rawUpdateTime=").append(rawUpdateTime);
