body {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	text-align:center;	
	background-color:#FFF;
	color:#555555;
	line-height:24px;
}
a:link {
	color: #555555;
	text-decoration: none;
}
a:visited {
	color: #555555;
	text-decoration: none;
}
a:hover {
	color: #ff0000;
	text-decoration: none;
}
a:active {
	color: #555555;
	text-decoration: none;
}

.red a:link {
	color: #ff0000;
	text-decoration: none;
}
.red a:visited {
	color: #ff0000;
	text-decoration: none;
}
.red a:hover {
	color: #ff0000;
	text-decoration: none;
}
.red a:active {
	color: #ff0000;
	text-decoration: none;
}

.title a:link {
	color: #ffffff;
	text-decoration: none;
}
.title a:visited {
	color: #ffffff;
	text-decoration: none;
}
.title a:hover {
	color: #ffffff;
	text-decoration: none;
}
.title a:active {
	color: #ffffff;
	text-decoration: none;
}
/*公共部分样式*/
