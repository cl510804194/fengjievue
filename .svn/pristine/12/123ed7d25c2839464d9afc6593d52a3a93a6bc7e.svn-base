	/** 服务名 */
	private final String service;
	
	/** 服务描述描述 */
	private final String message;
	
	/** 该服务是否需要登录 */
	private final boolean needLogin;
	/** 列表查询打印结果日志 false:不打印 */
	private final boolean noResultLog;
	
	/**
	 * 
	 * @param service 枚举值
	 * @param needLogin 是否需要登录
	 * @param message 枚举描述
	 */
	private AppServiceEnum(String service, boolean needLogin, boolean noResultLog, String message) {
		this.service = service;
		this.needLogin = needLogin;
		this.message = message;
		this.noResultLog = noResultLog;
	}
	
	/**
	 * 检查服务是否存在
	 * @param service
	 * @return
	 */
	public static boolean isExist(String service) {
		if (service == null) {
			return false;
		}
		for (AppServiceEnum _enum : values()) {
			if (_enum.getService().equals(service)) {
				return true;
			}
		}
		return false;
	}
	
	/**
	 * 通过service获取msg
	 * @param service 枚举值
	 * @return
	 */
	public static String getMsgByservice(String service) {
		if (service == null) {
			return null;
		}
		AppServiceEnum _enum = getByservice(service);
		if (_enum == null) {
			return null;
		}
		return _enum.getMessage();
	}
	
	/**
	 * 通过service判断是否需要登录
	 * @param service
	 * @return
	 */
	public static boolean isNeedLogin(String service) {
		
		AppServiceEnum _enum = getByservice(service);
		if (_enum == null) {
			return false;
		}
		return _enum.isNeedLogin();
	}
	
	/**
	 * 通过service判断是否需要打印全部结果日志
	 * @param service
	 * @return
	 */
	public static boolean printResultLog(String service) {
		AppServiceEnum _enum = getByservice(service);
		if (_enum == null) {
			return true;
		}
		return _enum.noResultLog;
	}
	
	/**
	 * 通过枚举<service>service</service>获得枚举
	 * 
	 * @param service
	 * @return AppServiceEnum
	 */
	public static AppServiceEnum getByservice(String service) {
		for (AppServiceEnum _enum : values()) {
			if (_enum.getService().equals(service)) {
				return _enum;
			}
		}
		return null;
	}
	
	/**
	 * 获取全部枚举
	 * 
	 * @return List<AppServiceEnum>
	 */
	public static java.util.List<AppServiceEnum> getAllEnum() {
		java.util.List<AppServiceEnum> list = new java.util.ArrayList<AppServiceEnum>(
			values().length);
		for (AppServiceEnum _enum : values()) {
			list.add(_enum);
		}
		return list;
	}
	
	/**
	 * 获取全部枚举值
	 * 
	 * @return List<String>
	 */
	public static java.util.List<String> getAllEnumservice() {
		java.util.List<String> list = new java.util.ArrayList<String>(values().length);
		for (AppServiceEnum _enum : values()) {
			list.add(_enum.getService());
		}
		return list;
	}
	
	/**
	 * 获取枚举service
	 * @param _enum
	 * @return
	 */
	public static String getservice(AppServiceEnum _enum) {
		if (_enum == null) {
			return null;
		}
		return _enum.getService();
	}
	
	public String getService() {
		return this.service;
	}
	
	public String getMessage() {
		return this.message;
	}
	
	public boolean isNeedLogin() {
		return this.needLogin;
	}
	
	public boolean isNoResultLog() {
		return this.noResultLog;
	}
	
}
