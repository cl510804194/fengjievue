/*对齐方式*/
.vm{vertical-align:middle;}
.vt{vertical-align:top;}
.vb{vertical-align:bottom;}
.text-r{text-align:right;}
.text-c{text-align:center;}
/*竖线*/
.line{color:#ccc; padding:0 10px;}
/*最小高度*/
.mh400{min-height:400px; _height:400px;}
/* 强制换行 */
/* 一般 block-level 元素 */
.sl-wrap, .sl-wrap td, .sl-wrap th{
	word-wrap:break-word;word-break:break-all;
}
/* table */
.sl-wrap-table{
	table-layout:fixed;
}

/* 强制不换行 */
.ws-no{ word-break:break-all; word-wrap:break-word; }

/* 页面位置 */
.g-full{ width:100%; }
.g-container{ margin:0 auto; width:1000px; }
.g-centerImg{ margin:0 auto; display:block; }
.g-ma{ margin:0 auto; }
.g-tlbr{ position:absolute; margin:auto; top:0; left:0; right:0; bottom:0;  }

/*层级*/
.g-zix5{ z-index:5; }
.g-zix6{ z-index:6; }
.g-zix7{ z-index:7; }
.g-zix8{ z-index:8; }
.g-zix9{ z-index:9; }

/*text-indent*/
.f-it1{ text-indent:1em; }
.f-it2{ text-indent:2em; }
.f-it3{ text-indent:3em; }
.f-it4{ text-indent:4em; }
.f-it5{ text-indent:5em; }

/*定位*/
.f-rel{ position:relative; }
.f-abs{ position:absolute; }
.f-fix{ position:fixed; }

/*使文字上下居中*/
.f-box{ display:table; }
.f-children{ vertical-align:middle; display:table-cell; }

/*显示影藏*/
.f-db{ display:block; }
.f-dn{ display:none; }

/*常用top，left*/
.g-t50p{ top:50%; }
.g-l50p{ left:50%; }
.g-bottom{ bottom:0; }
.g-top-l{ top:0; left:0; }
.g-top-r{ top:0; right:0; }
.g-bottom-l{ bottom:0; left:0; }
.g-bottom-r{ bottom:0; right:0; }
/*鼠标状态*/
.f-cu_p{ cursor:pointer; }

/*块级元素inline-block*/
a,b,span,em,i,strong{ display:inline-block; }

/*图片居中*/
.imgmiddle{ height:100%; display:inline-block; overflow:hidden; }

