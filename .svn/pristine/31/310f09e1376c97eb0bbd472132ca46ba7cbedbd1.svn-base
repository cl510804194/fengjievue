package com.yjf.esupplier.ws.gifamount.query.order;

import com.yjf.common.lang.util.money.Money;
import com.yjf.esupplier.ws.gifamount.enums.GiftMoneyTypeEnum;
import com.yjf.esupplier.ws.service.query.QueryPageBase;

public class UserCanUseGiftMoneyQueryOrder extends QueryPageBase {
    private static final long serialVersionUID = 4637443397492729265L;
    private Money amount;
    private GiftMoneyTypeEnum giftMoneyTypeEnum;
    private  long userId;

    public Money getAmount() {
        return amount;
    }

    public void setAmount(Money amount) {
        this.amount = amount;
    }

    public GiftMoneyTypeEnum getGiftMoneyTypeEnum() {
        return giftMoneyTypeEnum;
    }

    public void setGiftMoneyTypeEnum(GiftMoneyTypeEnum giftMoneyTypeEnum) {
        this.giftMoneyTypeEnum = giftMoneyTypeEnum;
    }

    public long getUserId() {
        return userId;
    }

    public void setUserId(long userId) {
        this.userId = userId;
    }

    @Override
    public String toString() {
        return "UserCanUseGiftMoneyQueryOrder{" +
                "amount=" + amount +
                ", giftMoneyTypeEnum=" + giftMoneyTypeEnum +
                ", userId=" + userId +
                '}';
    }
}
