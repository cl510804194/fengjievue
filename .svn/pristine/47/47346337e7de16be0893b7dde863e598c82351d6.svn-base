/* 公用样式 */
body { margin: 0px; border: 0px; font-size: 12px; font-family: "新宋体"; background-color: #fff;}
form { margin: 0px; padding: 0px; }
ul { list-style: none; margin: 0px; padding: 0px; }
img { border: 0px; }
a:link { color: #003278; text-decoration: none; }
a:visited { color: #003278; text-decoration: none; }
a:hover { color: #FF6600; text-decoration: underline; }
.frmrd {border:0px;}
.input raido {border:0px;}
.daohang {FONT-SIZE: 14px; COLOR:#fff; FONT-FAMILY:"新宋体";padding-left:25px;font-weight:bold;background:url(../image/daohang_bg630.jpg) no-repeat;vertical-align:middle;}
