.gwc01_2{ width:280px; color:#5b5b5b; float:left;font-size:12px; text-align:left; line-height:24px; vertical-align:middle; margin-left:300px; }
.gwc01_3{ width:50px; background:url(../images/help.gif) no-repeat left 50%;text-align:center; line-height:24px; vertical-align:middle; float:left;}
.gwc02{width:960px; background:#FFFFFF; height:60px;}
.gwc02_1{width:105px; height:60px;background:url(../images/logo_p.jpg) no-repeat left 50%; float:left}
.gwc02_2{width:840px;background:#FFFFFF; height:40px; margin-top:10px; margin-bottom:10px; display:inherit; font-size:18px; font-weight:bold; border-left:1px solid #666666; float:left; text-align:left; line-height:40px; vertical-align:middle;color:#0000; padding-left:5px;}
/*进货单导航开始 */
/*进货单导航开始 */
.gwc03{width:960px; background:#FFFFFF; height:80px;}
.flow-steps {margin:20px 0;overflow:hidden;}
.flow-steps li.current {background-color:#66c00b;color:#FFFFFF;}
.flow-steps li.done {background-color:#FFE6BA;background-position:100% -46px;color:#FF6600;}
.flow-steps li.current-prev {background-position:100% -23px;}
.flow-steps li.done .first {background-color:#FFE6BA;background-position:-12px -115px;}
.flow-steps .num5 li {width:165px;}
.flow-steps li {background:url(../images/flow_steps_bg.png) #E4E4E4 no-repeat scroll 100% 0;color:#404040;float:left;font-size:14px;font-weight:bold;height:23px;line-height:23px;overflow:hidden;padding:0 15px 0 0;text-align:center;}
.flow-steps li.current .first {background-color:#66c00b;background-position:-12px -92px;}
.flow-steps li .first {background:url(../images/flow_steps_bg.png) #E4E4E4 no-repeat scroll -12px -69px;}
.flow-steps li span, .flow-steps li strong {display:block;} 
.flow-steps li {color:#404040;font-size:14px;font-weight:bold;line-height:23px;text-align:center;}
.flow-steps li.last {background-position:100% -138px;}
/*进货单导航结束 */

/*进货单导航结束 */
