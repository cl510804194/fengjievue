	<!--会员统计根据时间 -->
	<select id="MS-MEMBER-STATISTICS-COUNT-BY-CONDITION" resultClass="com.yjf.esupplier.dal.dataobject.MemberStatisticsDO">
		SELECT IFNULL(COUNT(*),0),DATE_FORMAT(row_add_time,'%Y-%d-%m') FROM user_base_info 
		where 1=1
		<isNotEmpty property="startDate" prepend=" and ">
			<![CDATA[ row_add_time >= #startDate# ]]>
		</isNotEmpty>
		<isNotEmpty property="endDate" prepend=" and ">
			<![CDATA[ row_add_time <= #endDate# ]]>
		</isNotEmpty>
		GROUP BY DATE_FORMAT(row_add_time,'%Y-%d-%m');
	</select>

