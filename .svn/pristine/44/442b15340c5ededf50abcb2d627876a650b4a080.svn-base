package com.yjf.esupplier.ws.order;

import com.yjf.common.service.Order;
import com.yjf.esupplier.ws.enums.RecommendFriendStatusEnum;

import java.util.Date;

public class CreateRecommendFriendOrder extends ValidateOrderBase implements Order {
