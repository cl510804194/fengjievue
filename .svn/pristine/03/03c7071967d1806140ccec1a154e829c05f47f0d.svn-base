	<style type="text/css">
		.company{
			background-color: #f5eee0;
		}
		.company_cont{
			//height: 500px;			
		}
		.main-left{
			width: 200px;
			float: left;
		}
		.main-left ul li{
			margin-bottom: 1px;
		}
		.company_list{
			display: inline-block;
