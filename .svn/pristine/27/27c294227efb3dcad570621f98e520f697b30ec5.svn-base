BODY {
	MARGIN: 0px
}
P {
	MARGIN: 0px
}
BODY {
	BACKGROUND-COLOR: #fff; COLOR: #000
}
BODY {
	LINE-HEIGHT: 150%; FONT-FAMILY: "Verdana", "Arial", "Helvetica", "sans-serif"; FONT-SIZE: 12px
}
TABLE {
	LINE-HEIGHT: 150%; FONT-FAMILY: "Verdana", "Arial", "Helvetica", "sans-serif"; FONT-SIZE: 12px
}
INPUT {
	FONT-FAMILY: "Verdana", "Arial", "Helvetica", "sans-serif"; FONT-SIZE: 12px
}
SELECT {
	FONT-FAMILY: "Verdana", "Arial", "Helvetica", "sans-serif"; FONT-SIZE: 12px
}
TEXTAREA {
	FONT-FAMILY: "Verdana", "Arial", "Helvetica", "sans-serif"; FONT-SIZE: 12px
}
A:link {
	COLOR: #036; TEXT-DECORATION: none
}
A:visited {
	COLOR: #036; TEXT-DECORATION: none
}
A:hover {
	COLOR: #f60; TEXT-DECORATION: underline
}
A.menuChild:link {
	COLOR: #036; TEXT-DECORATION: none
}
A.menuChild:visited {
	COLOR: #036; TEXT-DECORATION: none
}
A.menuChild:hover {
	COLOR: #f60; TEXT-DECORATION: underline
}
A.menuParent:link {
	COLOR: #FFF; TEXT-DECORATION: none; font-size:14px; font-weight:bold; text-align:center;
}
A.menuParent:visited {
	COLOR: #FFF; TEXT-DECORATION: none;font-size:14px; font-weight:bold; text-align:center;
}
A.menuParent:hover {
	COLOR: #e79029; TEXT-DECORATION: none;font-size:14px; font-weight:bold; text-align:center;
}
TABLE.position {
	WIDTH: 100%
}
TR.position {
	BACKGROUND-COLOR: #f4f7fc; HEIGHT: 25px
}
TD.position {
	BORDER-BOTTOM: #adceff 1px solid; PADDING-LEFT: 20px; BORDER-RIGHT: #adceff 1px solid
}
TABLE.listTable {
	BACKGROUND-COLOR: #b1ceee; WIDTH: 98%
}
TR.listHeaderTr {
	TEXT-ALIGN: center; BACKGROUND-COLOR: #ebf4fd; HEIGHT: 25px; FONT-WEIGHT: bold
}
TR.listTr {
	TEXT-ALIGN: center; BACKGROUND-COLOR: #fff; HEIGHT: 25px
}
TR.listAlternatingTr {
	TEXT-ALIGN: center; BACKGROUND-COLOR: #fffdf0; HEIGHT: 25px
}
TR.listFooterTr {
	TEXT-ALIGN: center; BACKGROUND-COLOR: #ebf4fd; HEIGHT: 30px
}
TABLE.editTable {
	BACKGROUND-COLOR: #b1ceee; WIDTH: 98%
}
TR.editHeaderTr {
	BACKGROUND-COLOR: #ebf4fd; HEIGHT: 25px
}
TD.editHeaderTd {
	PADDING-LEFT: 50px; FONT-WEIGHT: bold
}
TR.editTr {
	HEIGHT: 30px
}
TD.editLeftTd {
	TEXT-ALIGN: center; BACKGROUND-COLOR: #fffdf0; WIDTH: 150px
}
TD.editRightTd {
	BACKGROUND-COLOR: #fff; PADDING-LEFT: 10px
}
TR.editFooterTr {
	BACKGROUND-COLOR: #ebf4fd; HEIGHT: 40px
}
TD.editFooterTd {
	PADDING-LEFT: 150px
}
.style1 {
	COLOR: #00ffff
}
H3 {
	PADDING-BOTTOM: 0.3em; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
P {
	PADDING-BOTTOM: 0.5em; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
.pane {
	POSITION: relative; PADDING-BOTTOM: 10px; PADDING-LEFT: 20px; PADDING-RIGHT: 20px; BACKGROUND: #edf5e1; BORDER-TOP: #c4df9b 2px solid; PADDING-TOP: 10px
}
.pane .delete {
	POSITION: absolute; TOP: 10px; CURSOR: pointer; RIGHT: 10px
}
.menuTr{background:url(../image/menu_files/menu_bt2.gif) no-repeat; text-align:left; padding-left:18px;}
.menuTitle{ text-align:center;}
